{
  "expo": {
    "name": "DescamClub",
    "slug": "DescamClub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/descamLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/descamLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.descam"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/descamLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/descamLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dba0e450-92c2-4e4b-b9a6-7b91ee5fed8c"
      }
    }
  }
}
